{
  "name": "visionai",
  "displayName": "Vision AI",
  "description": "Vision AI is an intelligent VSCode extension powered by Gemini Pro AI, designed to revolutionize your coding experience.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.1"
  },
  "categories": [
    "Other",
    "Programming Languages",
    "Education"
  ],
  "icon": "media/logo.png",
  "publisher": "ryangc93",
  "repository": {
    "type": "git",
    "url": "https://github.com/RyanGC93/vision-ai-vscode-ex.git"
  },
  "activationEvents": [
    "onCommand:vision.suggestComment",
    "onCommand:vision.explainSelection",
    "onCommand:vision.getSuggestedPlan",
    "onView:vision-sidebar"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vision-sidebar-view",
          "title": "Vision AI",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "vision-sidebar-view": [
        {
          "type": "webview",
          "id": "vision-sidebar",
          "name": "Vision AI",
          "icon": "media/icon.svg",
          "contextualTitle": "Vision"
        }
      ]
    },
    "commands": [
      {
        "command": "vision.explainSelection",
        "category": "Vision",
        "title": "Explain This"
      },
      {
        "command": "vision.suggestComment",
        "category": "Vision",
        "title": "Suggest Comment"
      },
      {
        "command": "vision.explainTest",
        "category": "Vision",
        "title": "Explain Test"
      },
      {
        "command": "vision.suggestTest",
        "category": "Vision",
        "title": "Suggest Test"
      },
      {
        "command": "vision.promptSelection",
        "category": "Vision",
        "title": "Prompt Selection"
      },
      {
        "command": "vision.getSuggestedPlan",
        "category": "Vision",
        "title": "Get Suggested Plan"
      },
      {
        "command": "vision.fileView",
        "category": "Vision",
        "title": "Open File View"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorTextFocus",
          "command": "vision.explainSelection",
          "group": "0_vision"
        },
        {
          "when": "editorTextFocus",
          "command": "vision.explainTest",
          "group": "0_vision"
        },
        {
          "when": "editorTextFocus",
          "command": "vision.suggestComment",
          "group": "0_vision"
        },
        {
          "when": "editorTextFocus",
          "command": "vision.promptSelection",
          "group": "0_vision"
        },
        {
          "when": "editorTextFocus",
          "command": "vision.getSuggestedPlan",
          "group": "0_vision"
        }
      ]
    },
    "keybindings": [
      {
        "command": "vision.explainTest",
        "key": "ctrl+shift+t",
        "mac": "cmd+shift+t",
        "when": "editorTextFocus"
      },
      {
        "command": "vision.explainSelection",
        "key": "ctrl+shift+e",
        "mac": "cmd+shift+e",
        "when": "editorTextFocus"
      },
      {
        "command": "vision.suggestComment",
        "key": "ctrl+shift+c",
        "mac": "cmd+shift+c",
        "when": "editorTextFocus"
      },
      {
        "command": "vision.promptSelection",
        "key": "ctrl+shift+m",
        "mac": "cmd+shift+m",
        "when": "editorTextFocus"
      },
      {
        "command": "vision.getSuggestedPlan",
        "key": "ctrl+shift+p",
        "mac": "cmd+shift+p",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "title": "Vision AI",
      "properties": {
        "vision.apiKey": {
          "type": "string",
          "default": "",
          "description": "Gemini AI API Key"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "rollup -c && webpack --mode production",
    "watch": "concurrently \"rollup -c -w\" \"webpack --mode development --watch\"",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@tsconfig/svelte": "^5.0.2",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "rollup": "^4.9.1",
    "rollup-plugin-svelte": "^6.0.0",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "svelte-preprocess": "^5.1.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "marked": "^11.1.0"
  }
}